    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0 || ^5.6",
        "phergie/phergie-irc-bot-react": "^2",
        "phergie/phergie-irc-plugin-react-url": "^3",
        "ext-json": "*"
    },
    "require-dev": {
	"phergie/phergie-irc-bot-react-development": "^1"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\YouTube\\": "src"
