{
    "name": "phergie/phergie-irc-plugin-react-youtube",
    "description": "Phergie plugin for providing information about YouTube videos",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "team@phergie.org",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/phergie/phergie-irc-plugin-react-youtube/issues",
